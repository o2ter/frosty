{
  "name": "frosty",
  "version": "1.0.0",
  "main": "dist/index",
  "module": "dist/index",
  "types": "dist/index",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./*": {
      "import": {
        "types": "./dist/*.d.ts",
        "default": "./dist/*.mjs"
      },
      "require": {
        "types": "./dist/*.d.ts",
        "default": "./dist/*.js"
      }
    }
  },
  "files": [
    "dist/*",
    "packages/*"
  ],
  "repository": "https://github.com/o2ter/frosty.git",
  "author": "Susan Cheng <susan.doggie@gmail.com>",
  "license": "MIT",
  "scripts": {
    "gen-data": "node ./scripts/gen-data.mjs",
    "clean": "rm -rf ./dist/*",
    "rollup": "yarn clean && rollup -c",
    "test": "jest"
  },
  "dependencies": {
    "@o2ter/utils-js": "^0.0.19",
    "@types/jsdom": "^21.1.7",
    "@types/lodash": "^4.17.16",
    "autoprefixer": "^10.4.21",
    "csstype": "^3.1.3",
    "jsdom": "^26.0.0",
    "lodash": ">=4.17.21",
    "myers.js": "^0.0.19",
    "nextick": "^0.0.2",
    "postcss": "^8.5.3",
    "postcss-js": "^4.0.1",
    "postcss-nested": "^7.0.2"
  },
  "devDependencies": {
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/postcss-js": "^4.0.4",
    "@webref/css": "^6.20.4",
    "@webref/elements": "^2.4.0",
    "@webref/events": "^1.18.3",
    "@webref/idl": "^3.61.1",
    "core-js": "^3.41.0",
    "dotenv-webpack": "^8.1.0",
    "html-element-attributes": "^3.4.0",
    "jest": "^29.7.0",
    "rollup": "^4.22.4",
    "rollup-plugin-dts": "^6.1.1",
    "svg-element-attributes": "^2.1.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.7.3",
    "webidl2": "^24.4.1"
  }
}
