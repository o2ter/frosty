{
  "name": "frosty",
  "version": "1.0.0",
  "main": "dist/index",
  "module": "dist/index",
  "types": "dist/index",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./*": {
      "import": "./dist/*.mjs",
      "require": "./dist/*.js"
    }
  },
  "files": [
    "dist/*",
    "packages/*"
  ],
  "repository": "https://github.com/o2ter/frosty.git",
  "author": "Susan Cheng <susan.doggie@gmail.com>",
  "license": "MIT",
  "scripts": {
    "gen-data": "node ./scripts/gen-data.mjs",
    "clean": "rm -rf ./dist/*",
    "rollup": "yarn clean && rollup -c",
    "test": "jest",
    "start": "npx frosty run -d -w -s ./src tests/server/app.tsx"
  },
  "bin": {
    "frosty": "./packages/frosty-cli/bin/frosty.sh"
  },
  "dependencies": {
    "@o2ter/utils-js": "^0.0.19",
    "@types/jsdom": "^21.1.7",
    "@types/lodash": "^4.17.16",
    "csstype": "^3.1.3",
    "jsdom": "^27.0.0",
    "lodash": ">=4.17.21",
    "myers.js": "^0.0.20",
    "nextick": "^0.0.2",
    "postcss": "^8.5.6",
    "postcss-js": "^5.0.0",
    "postcss-nested": "^7.0.2"
  },
  "devDependencies": {
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/jest": "^30.0.0",
    "@types/postcss-js": "^4.0.4",
    "@webref/css": "^7.0.11",
    "@webref/elements": "^2.4.0",
    "@webref/events": "^1.18.3",
    "@webref/idl": "^3.67.1",
    "core-js": "^3.45.1",
    "dotenv-webpack": "^8.1.0",
    "frosty-cli": "./packages/frosty-cli",
    "html-element-attributes": "^3.4.0",
    "jest": "^30.1.3",
    "rollup": "^4.50.2",
    "rollup-plugin-dts": "^6.1.1",
    "svg-element-attributes": "^2.1.0",
    "ts-jest": "^29.4.2",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.7.3",
    "webidl2": "^24.4.1"
  }
}
