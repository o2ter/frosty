{
  "name": "frosty",
  "version": "1.0.0",
  "main": "dist/index",
  "module": "dist/index",
  "types": "dist/index",
  "files": [
    "dist/*"
  ],
  "repository": "https://github.com/o2ter/frosty.git",
  "author": "Susan Cheng <susan.doggie@gmail.com>",
  "license": "MIT",
  "scripts": {
    "gen-data": "node ./scripts/gen-data.mjs",
    "clean": "rm -rf ./dist/*",
    "rollup": "yarn clean && rollup -c",
    "test": "jest",
    "watch:build": "./tests/server/bin/build.sh -w",
    "watch:start": "until [ -f ./tests/server/dist/server.js ]; do sleep 1; done && nodemon --watch ./tests/server/dist ./tests/server/dist/server.js",
    "start": "rm -rf ./tests/server/dist/* && npm-run-all --parallel watch:*"
  },
  "dependencies": {
    "@o2ter/utils-js": "^0.0.19",
    "autoprefixer": "^10.4.21",
    "csstype": "^3.1.3",
    "lodash": ">=4.17.21",
    "myers.js": "^0.0.19",
    "nextick": "^0.0.2",
    "postcss-js": "^4.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@babel/preset-env": "^7.24.5",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.24.1",
    "@o2ter/server-js": "^0.0.20",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/jsdom": "^21.1.7",
    "@types/lodash": "^4.17.16",
    "@types/postcss-js": "^4.0.4",
    "@webref/elements": "^2.4.0",
    "babel-jest": "^29.7.0",
    "babel-loader": "^10.0.0",
    "core-js": "^3.41.0",
    "jest": "^29.7.0",
    "jsdom": "^26.0.0",
    "nodemon": "^3.1.9",
    "npm-run-all": "^4.1.5",
    "rollup": "^4.22.4",
    "rollup-plugin-dts": "^6.1.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.7.3",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1"
  }
}
